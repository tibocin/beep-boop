# Beep-Boop Environment Configuration
# Copy this file to .env and fill in your values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Digi-Core Integration (Primary RAG Backend)
DIGI_CORE_API_URL=http://localhost:8000
DIGI_CORE_API_KEY=your_digi_core_api_key_here
DIGI_CORE_ENABLED=true
DIGI_CORE_MIN_CONFIDENCE=0.3

# Alternative RAG Backends (Fallback)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment

# Application Settings
DEBUG=false
LOG_LEVEL=INFO 